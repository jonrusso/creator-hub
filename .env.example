# Supabase Configuration for Creator Hub
# Copy this file to .env.local and fill in your Supabase credentials

# Supabase Project URL (from Project Settings > API)
VITE_SUPABASE_URL=

# Supabase Anonymous Key (from Project Settings > API > anon/public)
VITE_SUPABASE_ANON_KEY=

# Set to 'false' to use Supabase instead of mock data
# When Supabase credentials are not configured, mock data is used automatically
VITE_USE_MOCK=true
